  "engines": {
    "node": ">=22.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sequencemedia/music-library.git"
  },
  "homepage": "https://github.com/sequencemedia/music-library#readme",
  "bugs": {
    "url": "https://github.com/sequencemedia/music-library/issues"
  },
  "scripts": {
